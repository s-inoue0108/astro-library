---
import Layout from "../layouts/Layout.astro";
import PageTitle from "../components/blog/background/PageTitle.astro";
import PageTitleForPc from "../components/blog/background/PageTitleForPc.astro";
import Recaptcha from "../components/common/form/Recaptcha";
import { svgPaths, svgViewBoxes } from "../lib/svg/paths";
---

<Layout title={`お問い合わせ | ${import.meta.env.APP_NAME}`} description="お問い合わせ">
	<PageTitle
		title="お問い合わせ"
		svgIconPath={svgPaths.envelope}
		svgViewBox={svgViewBoxes.envelope}
	/>
	<PageTitleForPc
		title="お問い合わせ"
		svgIconPath={svgPaths.envelope}
		svgViewBox={svgViewBoxes.envelope}
	/>
	<Recaptcha client:load />
</Layout>
